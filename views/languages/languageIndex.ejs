<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <table id="languageIndex">
        <thead>
          <tr>
            <th>Language</th>
          </tr>
        </thead>
        <tbody>
          <% languages.forEach(function(l) { %>
            <tr>
              <td><%= l.language %></td>
              <td><a href="/languages/<%= l._id %>">Learn More</a></td>
              <td><a href="/languages" >Delete</a></td>
   
            </tr>
            <% }); %>
          </tbody>
        </table>
    
</body>
</html>

